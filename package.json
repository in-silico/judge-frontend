{
  "name": "judge",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build-js": "browserify -t [ babelify --presets [ react ] ] browser/main.js -o public/js/bundle.js",
    "watch-js": "watchify -t [ babelify --presets [ react ] ] browser/main.js -o public/js/bundle.js -dv",
    "start": "npm run build-js && node ./bin/www",
    "start-dev": "export NODE_ENV=development && npm run watch-js & node ./bin/www"
  },
  "dependencies": {
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "body-parser": "~1.13.2",
    "browserify": "^12.0.1",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "ejs": "~2.3.3",
    "express": "~4.13.1",
    "morgan": "~1.6.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "serve-favicon": "~2.3.0",
    "watchify": "^3.7.0"
  }
}
